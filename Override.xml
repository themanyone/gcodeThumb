<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
    <mime-type type="text/x.gcode">
        <sub-class-of type="text/plain"/>
        <comment>Gcode file</comment>
        <icon name="gcode"/>
        <glob-deleteall/>
        <glob pattern="*.gcode"/>
        <glob pattern="*.g"/>
        <glob pattern="*.gc"/>
        <glob pattern="*.nc"/>
    </mime-type>
</mime-info>

